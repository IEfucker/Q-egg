{
  "name": "q-egg",
  "version": "1.0.0",
  "description": "q server based on eggjs",
  "dependencies": {
    "egg": "2.10.0",
    "egg-passport": "^2.0.1",
    "egg-passport-github": "^1.0.0",
    "egg-scripts": "^2.5.0",
    "egg-sequelize": "^4.0.7",
    "lodash": "^4.17.10",
    "mysql2": "^1.6.1"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "egg-bin": "^4.3.7",
    "egg-mock": "^3.19.2",
    "eslint": "^4.18.1",
    "eslint-config-egg": "^7.0.0",
    "factory-girl": "^5.0.2",
    "sequelize-cli": "^4.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "autod": "autod",
    "lint": "eslint .",
    "dev": "egg-bin dev",
    "test": "egg-bin test",
    "cov": "egg-bin cov",
    "ci": "npm run lint && NODE_ENV=test npx sequelize db:migrate && npm run cov",
    "migrate:new": "sequelize migration:create",
    "migrate:up": "sequelize db:migrate",
    "migrate:down": "sequelize db:migrate:undo",
    "seed:new": "sequelize seed:generate",
    "seed:up": "sequelize db:seed:all",
    "seed:down:all": "sequelize db:seed:undo:all"
  },
  "ci": {
    "version": "8"
  },
  "eslintIgnore": [
    "coverage",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "files": [
    "lib",
    "index.js"
  ],
  "author": "Jack Bai",
  "private": true
}
